{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
        "*": ["./src/*"]
      },
      "lib": ["DOM", "ESNext"]
    }
  }
  